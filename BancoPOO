-- Database: BancoPOO

-- DROP DATABASE IF EXISTS "BancoPOO";

CREATE DATABASE "BancoPOO"
    WITH
    OWNER = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'pt_BR.UTF-8'
    LC_CTYPE = 'pt_BR.UTF-8'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;
    
CREATE TABLE cliente(
    id int;
    nome varchar,
    email varchar,
    telefone bigint,
        PRIMARY KEY(id)
)

SELECT * FROM cliente;